func isprime(x: int) {

    let cond: int = 1;

    if cond % 2 == 0{
        cond = 0;
    }

    for i:int = 3; i < x / 2 && cond == 1; i = i + 2 {
        if (x % i == 0) {
            cond = 0;
        }
    }

    if cond == 0 {
        print("Is not prime");
    }
    else {
        print("Is prime");
    }
    return;
}

func main() {

    let x;
    read(x);
    if x >= 0 {
        isprime(x);
    }
    else {
        print("Invalid number");
    }

    return;
}