
/*
    For loops declared control variables are mutable by default as is their intended use
*/


func main(){
    let mut x = 3;
    let y = x + 10;

    if x >= 0 {
        print("Is positive");
    }

    if y >= 0 {
        print("Is positive");
    }
    else {
        print("Is negative");
    }

    for i: int = 0; i < y; i = i + 1{
        x = x + y;
    }

    print(x);

    return;
}



